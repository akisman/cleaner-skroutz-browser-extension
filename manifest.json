{
    "manifest_version": 2,
    "name": "Cleaner Skroutz",
    "version": "1.0.0",
  
    "description": "This extension removes ads and promotional elements from the webpage skroutz.gr for a cleaner browsing experience.",
    "homepage_url": "https://github.com/akisman/cleaner-skroutz-browser-extension",
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.skroutz.gr/*"],
        "js": ["script.js"],
        "css": ["style.css"]
      }
    ]
}